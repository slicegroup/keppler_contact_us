%tr{ id: "row-#{message_setting.id}" }
	- if can? :destroy_multiple, Message_setting
		%td.item-check{style:"width: 10%"}
			= link_to "", message_setting_path(message_setting), remote: true, class: "show-row"
			%input{ type:"checkbox", class: "filled-in", id: "checkbox-#{message_setting.id}", 'checklist-model'=>"check", 'checklist-value'=>"#{message_setting.id}", "ng-click"=>"selectByCheck(#{@objects.size})"}
			%label{ for: "checkbox-#{message_setting.id}" }

		%td{style:"width: 30%"}
			= link_to "", message_setting_path(message_setting, search: @query), remote: true, class: "show-row"
			%span
				= message_setting.mailer_to
		%td{style:"width: 30%"}
			= link_to "", message_setting_path(message_setting, search: @query), remote: true, class: "show-row"
			%span
				= message_setting.recaptcha_pv
		%td{style:"width: 30%"}
			= link_to "", message_setting_path(message_setting, search: @query), remote: true, class: "show-row"
			%span
				= message_setting.recaptcha_pb
								
	%td{style:"width: 5%"}		
		= link_to message_setting_path(message_setting), class: "btn-floating waves-effect btn-flat" do
			= material_icon.md_24.more_horiz.css_class("md-dark")